{
	"name": "oojs",
	"version": "7.0.1",
	"description": "Power for object oriented JavaScript libraries.",
	"keywords": [
		"oo",
		"oop",
		"class",
		"inheritance",
		"library"
	],
	"homepage": "https://www.mediawiki.org/wiki/OOjs",
	"repository": {
		"type": "git",
		"url": "https://gerrit.wikimedia.org/g/oojs/core"
	},
	"license": "MIT",
	"main": "dist/oojs.js",
	"files": [
		"dist/",
		"AUTHORS.txt",
		"README.md",
		"LICENSE-MIT"
	],
	"scripts": {
		"lint": "eslint --cache .",
		"build-dev": "grunt build-dev",
		"build-release": "grunt build-release",
		"doc": "jsdoc -c .jsdoc.json ",
		"test": "npm run build-dev && karma start && qunit --require ./tests/setup-node tests/unit/ && npm run lint",
		"jenkins": "npm run test && karma start karma.conf.sauce.js && npm run doc"
	},
	"devDependencies": {
		"eslint-config-wikimedia": "0.25.0",
		"grunt": "1.6.1",
		"grunt-contrib-clean": "2.0.1",
		"grunt-contrib-concat": "2.1.0",
		"grunt-contrib-uglify": "5.2.2",
		"jsdoc": "3.6.10",
		"jsdoc-class-hierarchy": "1.1.2",
		"jsdoc-wmf-theme": "0.0.5",
		"karma": "6.3.18",
		"karma-chrome-launcher": "3.1.0",
		"karma-coverage": "2.0.3",
		"karma-firefox-launcher": "2.1.2",
		"karma-qunit": "4.1.2",
		"karma-remap-istanbul": "0.6.0",
		"karma-sauce-launcher": "4.3.6",
		"qunit": "2.19.4"
	}
}
